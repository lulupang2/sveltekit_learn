{
  "name": "front-svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vite dev",
    "prod": "NODE_ENV=production vite dev",
    "build": "vite build",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@skeletonlabs/skeleton": "^2.4.0",
    "@skeletonlabs/tw-plugin": "^0.2.3",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-netlify": "^2.0.8",
    "@sveltejs/kit": "^1.27.3",
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.69.5",
    "svelte": "next",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.0",
    "unplugin-icons": "^0.17.1",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@tanstack/svelte-query": "latest",
    "@tanstack/svelte-query-devtools": "^5.4.3",
    "@tiptap/core": "^2.1.12",
    "@tiptap/extension-bubble-menu": "^2.1.12",
    "@tiptap/extension-floating-menu": "^2.1.12",
    "@tiptap/extension-image": "^2.1.12",
    "@tiptap/extension-placeholder": "^2.1.12",
    "@tiptap/pm": "^2.1.12",
    "@tiptap/starter-kit": "^2.1.12",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "remixicon": "^3.5.0",
    "svelte-tiptap": "^1.1.2"
  }
}
